{
  "name": "models-blog-models",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/models/blog-models/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/models/blog-models/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/models/blog-models/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "db:lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma validate --schema ./schema.prisma",
        "cwd": "libs/models/blog-models/prisma"
      }
    },
    "db:migrate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma migrate dev --schema ./schema.prisma --skip-generate --skip-seed",
        "cwd": "libs/models/blog-models/prisma"
      }
    },
    "db:reset": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma migrate reset --schema ./schema.prisma --force --skip-generate --skip-seed",
        "cwd": "libs/models/blog-models/prisma"
      }
    },
    "db:generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma generate --schema ./schema.prisma",
        "cwd": "libs/models/blog-models/prisma"
      }
    },
    "db:seed": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx ts-node seed.ts",
        "cwd": "libs/models/blog-models/prisma"
      }
    }
  },
  "tags": []
}
